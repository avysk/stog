<form id="login-form">
<fieldset>
<div>
<label for="login">Login:</label>
<input type="text" name="login" id="login" placeholder="Your login" mand_="true" required="required"/>
</div>
<div>
<label for="password">Password:</label>
<input type="password" name="password" id="password" placeholder="Your password" mand_="true" required="required"/>
</div>
<error-msg type_="xmls" param_="true" optional_="true"/>
<input type="submit" name="submit" value="Login"/>
</fieldset>
</form>